   "source": [
    "def fact(num):\n",
    "    factorial = 1\n",
    "    for fact in range(1,num+1):\n",
    "        factorial *=  fact\n",
    "    print (f\"The factorial of {num} is :{factorial}\")\n",
    "    return factorial\n",
    "fact(5)"
   ]
